{
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
      "*": ["src/*"]
    },
    "declaration": false,
    "sourceMap": true,
    "noImplicitAny": true,
    "removeComments": true,
    "preserveConstEnums": true,
    "strictPropertyInitialization": false,
    "skipLibCheck": true,
    "jsx": "react",
    "moduleResolution": "node",
    "target": "es6",
    "lib": ["dom", "es6"],
    "importHelpers": false
  },
  "files": ["src/index.tsx"],
  "include": ["node_modules"]
}
